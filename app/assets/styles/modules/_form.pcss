
label {
  color: white;
}
.wFormWebPage {
  background-color: #414980;
}

.wForm .oneField {
  border: 2px solid transparent;
  padding:0;
  box-sizing: border-box;
}
.wFormContainer {
  font-family: 'Lato', sans-serif !important;
  background: none !important;
  border:none !important;
}
.wFormTitle {
  display: none;
}
.htmlSection {
  display: none;
}
.wFormContainer input[type="text"]:focus, .wFormContainer textarea:focus, .wFormContainer select:focus {
  outline: none !important;
  border: none !important;
  background: white;
  color: black;

  -webkit-transition: background-color 500ms linear;
  -moz-transition: background-color 500ms linear;
  -o-transition: background-color 500ms linear;
  -ms-transition: background-color 500ms linear;
  transition: background-color 500ms linear;

}
.wFormContainer textarea{
  resize:none !important;
  height: 90px !important;
}


.wFormContainer {
  border: none ;
  background: none ;
}

.wFormContainer label {
  font-size: 14px !important;
  color: rgba(255, 255, 255, 0.61) !important;
  padding: 2px 4px;
  font-weight: normal;
}

.wFormContainer label {

  white-space: normal;
}
.wFormContainer span{
  color: rgba(255, 255, 255, 0.61) !important;
}



.inputWrapper, .inputWrapper div {
  width: 100% !important;
}


.wFormContainer input[type="text"],  .wFormContainer textarea, .wFormContainer select {
  font-size: 14px !important;
  color: white ;
  width: 100% !important;
  background: #343a66;
  padding: 5px 10px;
  height: 40px;
  border: none !important;
  font-weight: lighter;
}

.wFormContainer select {
  border: 0 !important;
  -webkit-appearance: none;
  -moz-appearance: none;
  text-indent: 0.01px;
  text-overflow: "";
}

.wFormContainer select option {
  background-color: white;
}


.wFormContainer .oneField {
  width: 100%;
  height: auto;

  padding: 10px ;
  /* float: left; */
  border: none!important;
  /* margin-left: 1%; */
  /* margin-right: 1%; */
  margin: auto;
}

.wForm div > div.oneField, .wForm fieldset > div.oneField, .wForm form > div.oneField, .wForm fieldset, .wForm .matrixLayout, .wForm .gridLayout, .wForm h4, .wForm .wfPagingButtons, .wForm .actions {
  margin-top:0 !important;
}


.wForm .inputWrapper .delimiterBefore {
  color: white;
}

.wFormContainer input[type=submit] {
  letter-spacing: -1px;
  font-size: 16px !important;
  max-width: 300px;

  display: block;
  padding: 20px 20px;
  text-align: center;
  text-transform: uppercase;
  color: white;
  border-radius: 5px;
  margin:auto;
  background-color: #ed174f;
  text-decoration: none;

  cursor: pointer;
  outline: none !important;
  border: none !important;

  white-space: normal;

  border-radius: 0;

  -webkit-transition: background-color 200ms linear;
  -moz-transition: background-color 200ms linear;
  -o-transition: background-color 200ms linear;
  transition: background-color 200ms linear;


  -webkit-transition: color 200ms linear;
  -moz-transition: color 200ms linear;
  -o-transition: color 200ms linear;
  transition: color 200ms linear;

}


.wFormContainer input[type=submit]:hover {
  background-color: #c1d82f;
  color: black;

}
.wForm .primaryAction {
  background-image: none !important;
}

.lengthIndicator {
  display: none !important;
}

.wFormContainer .errMsg {
  color: #CC3333;
  right: 0;
  position: absolute;
}

.oneField {
  position: relative;
}

#tfa_7-D {
  width: 98%;
  min-height: 142px;
}

.supportInfo {
  display: none;
}
@media only screen and (max-width: 990px) {

  .wFormContainer .oneField {
    width: 98%;
  }

  .wFormContainer input[type=submit] {
    float: none;
    margin: auto;


  }

}
